{
    "name": "olekjs/laravel-athena-pdf",
    "description": "PDF",
    "keywords": [
        "olekjs",
        "laravel-athena-pdf"
    ],
    "homepage": "https://github.com/olekjs/laravel-athena-pdf",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "ChangeMe",
            "email": "changeme@changeme.tld",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.8.*"
    },
    "require-dev": {
        "orchestra/testbench": "^3.5",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Olekjs\\LaravelAthenaPdf\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Olekjs\\LaravelAthenaPdf\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Olekjs\\LaravelAthenaPdf\\LaravelAthenaPdfServiceProvider"
            ],
            "aliases": {
                "LaravelAthenaPdf": "Olekjs\\LaravelAthenaPdf\\LaravelAthenaPdfFacade"
            }
        }
    }
}
